		  <!-- Wall Panel View -->

	<include name="Viewtype-Walls">
		<control type="group">
			<visible>Control.IsVisible(56)</visible>
			<animation type="Conditional" reversible="false" condition="!Window.IsVisible(movieinformation) + !Window.IsVisible(musicinformation)">
					<effect type="fade" start="0" end="100" time="400" />
					<effect type="zoom" start="90" end="100" time="400" center="640,360" tween="sine" easing="out" />
			</animation>
			<animation type="Conditional" reversible="false" condition="Window.IsVisible(movieinformation) | Window.IsVisible(musicinformation)">
					<effect type="zoom" start="100" end="90" time="0" center="640,360" tween="sine" easing="out" />
					<effect type="fade" start="100" end="0" time="0" />
			</animation>
			<include condition="!Skin.HasSetting(slantedwall)">wallpanelanimation</include>
			<include condition="Skin.HasSetting(slantedwall)">wallpanelslantanimation</include>
	<!-- New info at top of panels  ends line 180 -->
		<!-- animations for the next three groups -->
		<control type="group">
			<animation type="WindowOpen" reversible="false">
					<effect type="fade" start="20" end="100" time="1500" delay="500" />
					<effect type="slide" start="0,70" end="0,0" time="1000" delay="500" tween="back" easing="out" />
			</animation>
			<animation type="Visible" reversible="false">
					<effect type="fade" start="20" end="100" time="1500" delay="500" />
					<effect type="slide" start="0,70" end="0,0" time="1000" delay="500" tween="back" easing="out" />
			</animation>
			<animation type="WindowClose" reversible="false">
					<effect type="fade" start="100" end="0" time="450" delay="0" />
					<effect type="slide" start="0,0" end="0,-70" time="350" delay="50" tween="back" easing="out" />
			</animation>
			<animation type="Hidden" reversible="false">
					<effect type="fade" start="100" end="0" time="450" delay="0" />
					<effect type="slide" start="0,0" end="0,-70" time="350" delay="50" tween="back" easing="out" />
			</animation>
