<includes>
		<include name="debug">
				<!-- Info Lables Underlay for FPS and Memory -->
				<control type="image">
						<posx>0</posx>
						<posy>93r</posy>
						<height>20</height>
						<width>1280</width>
						<texture>overlays/debug/loadedxml.png</texture>
						<colordiffuse>88FFFFFF</colordiffuse>
						<aspectratio>stretch</aspectratio>
						<visible>Skin.HasSetting(Debuginfo)</visible>
				</control>
				<control type="label">
						<posx>10</posx>
						<posy>99r</posy>
						<width>1280</width>
						<font>font13</font>
						<label>$LOCALIZE[31994] $INFO[System.FPS] - $LOCALIZE[31992] $INFO[System.FreeMemory]</label>
						<visible>Skin.HasSetting(Debuginfo)</visible>
				</control>
